# API Keys for CAD generation services
# Copy this file to .env and add your actual API keys
# NEVER commit .env file to version control

# Zoo.dev API key
# Get your key from: https://zoo.dev/account/api-keys
ZOO_API_KEY=your_zoo_dev_api_key_here

# Adam.new API key
# Get your key from: https://adam.new/settings/api
ADAM_API_KEY=your_adam_new_api_key_here

# Optional: Set to true for testing without API calls
MOCK_MODE=false
